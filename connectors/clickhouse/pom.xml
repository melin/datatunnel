    <artifactId>datatunnel-connector-clickhouse</artifactId>
    <name>Datatunnel : Connectors : Clickhouse</name>

    <dependencies>
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
        </dependency>

        <dependency>
            <groupId>com.clickhouse.spark</groupId>
